// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  name     String
  email    String  @unique
  password String
}

model countries {
  id       Int    @id @default(autoincrement())
  name     String @unique
  level    Int
}

model groups {
  id     Int @id @default(autoincrement())
  name  String @unique
  number Int @unique
}